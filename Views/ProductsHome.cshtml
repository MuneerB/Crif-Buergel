@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "Pages.cshtml";
}
@Umbraco.RenderMacro("InnerBanner")
<section class="main-content">
    <div class="container">
        <div class="row">
            <div class="inner-container">
                <div class="prdct-list-section lrg-sprtr-top">
                    <aside class="col-sm-3 filter-section">
                        @Umbraco.RenderMacro("ProductsFilter")
                    </aside>
                    @Umbraco.RenderMacro("ProductsFilterAjaxHandler")
                </div>
            </div>
        </div>
    </div>

</section>
@section innerJs{
	<script type="text/javascript" src="/scripts/ajax/productsAjaxHandler.js"></script>
}