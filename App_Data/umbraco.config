      <consumerInternal id="1656" key="f0bc4bba-54c2-4ef4-b71f-004a50eca7d0" parentID="1273" level="3" creatorID="0" sortOrder="3" createDate="2016-10-20T15:35:28" updateDate="2016-12-13T11:27:17" nodeName="CRIF für Konsumenten: Die Vorteile" urlName="crif-fuer-konsumenten-die-vorteile" path="-1,1059,1273,1656" isDoc="" nodeType="1268" creatorName="CrifAustria" writerName="CrifAustria" writerID="0" template="1267" nodeTypeAlias="consumerInternal">
