      <industrySingle id="1066" key="da48770d-13a7-4543-abd7-8a292b246844" parentID="1060" level="3" creatorID="0" sortOrder="1" createDate="2016-08-12T15:38:42" updateDate="2017-02-14T10:08:19" nodeName="Gewerbe / Handwerk" urlName="gewerbe-handwerk" path="-1,1059,1060,1066" isDoc="" nodeType="1062" creatorName="CrifAustria" writerName="Kavukcu Atilla" writerID="2" template="1221" nodeTypeAlias="industrySingle">
