      <linkBoxes><![CDATA[{"fieldsets":[{"properties":[{"alias":"title","value":"Alle Infos auf einem Blick - und das kostenlos!"},{"alias":"link","value":"[\r\n  {\r\n    \"id\": \"1687\",\r\n    \"name\": \"Selbstauskunft\",\r\n    \"url\": \"/konsumenten/selbstauskunft/\",\r\n    \"icon\": \"icon-company color-yellow\"\r\n  }\r\n]"},{"alias":"shortDescription","value":"<p>Vollen Zugriff auf alle Informationen, die bei CRIF zu Ihrer Person gespeichert sind.</p>"}],"alias":"linkBoxes","disabled":false,"id":"901f709c-8f4d-425d-a777-677dc8e5ac1d","releaseDate":null,"expireDate":null,"allowedMemberGroups":""},{"properties":[{"alias":"title","value":"Sie brauchen Infos zu einem Unternehmen?"},{"alias":"shortDescription","value":"<p>Nutzen Sie unser Online Portal iCRIF und erhalten Sie sofort Wirtschaftsinformationen zu über 500.000 Unternehmen.</p>"},{"alias":"link","value":"[\r\n  {\r\n    \"name\": \"iCRIF\",\r\n    \"url\": \"http://www.icrif.at/de\",\r\n    \"target\": \"_blank\",\r\n    \"icon\": \"icon-link\"\r\n  }\r\n]"}],"alias":"linkBoxes","disabled":false,"id":"9ce5b0c5-60ba-40c1-82ae-c9df72c647a2","releaseDate":null,"expireDate":null,"allowedMemberGroups":""}]}]]></linkBoxes>
